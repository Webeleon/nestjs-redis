{
  "name": "@webeleon/nestjs-redis",
  "version": "0.0.5",
  "description": "A redis client for NestJS",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "patch": "npm run build && npm version patch && npm publish",
    "format": "prettier --write 'src/**/*.ts'",
    "build": "tsc",
    "generate-doc": "typedoc",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Webeleon/nestjs-redis"
  },
  "bugs": {
    "url": "https://github.com/Webeleon/nestjs-redis/issues"
  },
  "contributors": [
    {
      "name": "Julien Prugne",
      "email": "julien@webeleon.dev"
    }
  ],
  "keywords": [
    "webeleon",
    "npm",
    "typescript",
    "NestJS",
    "redis"
  ],
  "author": "Julien Prugne<juliene@webeleon.dev>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@types/jest": "27.4.1",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.5",
    "prettier": "2.5.1",
    "standard-version": "9.3.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "typedoc": "0.22.12",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@nestjs/common": "^8.3.1",
    "@nestjs/core": "^8.3.1",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4"
  },
  "lint-staged": {
    "*.{ts}": "prettier --write"
  }
}
